<h1>My Lists</h1>

<div class="row">
  <div class="col-md-8">
    <% @lists.each do |list| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
         <%= link_to list.title, list_path(list) %>
          </h4>

        </div>
      </div>
    <% end %>

  </div>
</div>

<%= link_to "New To-do List", new_list_path, class: 'btn btn-success' %>